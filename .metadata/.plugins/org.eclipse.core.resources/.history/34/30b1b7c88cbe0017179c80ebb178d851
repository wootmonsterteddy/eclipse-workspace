#include <stdio.h>

int main(void)
{
	typedef struct
	{
		unsigned int val;
		int *next;
	}listEntry;

	listEntry e1,e2,e3,e4,e5;
	listEntry *ptr = &e1;

	e1.val = 1;
	e2.val = 2;
	e3.val = 3;
	e4.val = 4;
	e5.val = 5;
	e1.next = &e2;
	e2.next = &e3;
	e3.next = &e4;
	e4.next = &e5;
	e5.next = 0;

	while(*ptr)
	{
		printf("%iu",ptr->val);
		ptr++;
	}
}
