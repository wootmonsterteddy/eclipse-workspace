#include <stdio.h>

int main(void)
{
	typedef struct
	{
		unsigned short age;
		char *name;
	}person;

	person person1, person2, person3;

	person1.name = "Erik";
	person1.age = 24;
	person2.name = "Anna";
	person2.age = 30;
	person3.name = "Allan";
	person3.age = 91;

	printf("%hu",person2.age);
}
